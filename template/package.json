{
    "name": "CREATE_RIO_FRONTEND_appName",
    "description": "A frontend based on @rio-cloud/create-frontend",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "start": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "test": "vitest run",
        "test:ci": "vitest run --reporter=junit --outputFile.junit=./results/junit.xml",
        "test-dev": "vitest",
        "coverage": "vitest run --coverage",
        "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
        "lint-fix": "npm run lint -- --fix",
        "audit": "npm audit --production --registry https://registry.npmjs.org --parseable",
        "format-code": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
        "format-code-on-commit": "npm run format-code && git add",
        "pull-translations": "phrase pull --access_token $PHRASEAPP_ACCESS_TOKEN && npm run merge-translations",
        "merge-translations": "node mergeTranslations.js && rimraf src/features/translations/temp",
        "cypress-open": "cypress open",
        "cypress-run": "cypress run",
        "cypress-ci": "start-server-and-test start http://localhost:3000 cypress-run"
    },
    "dependencies": {
        "@redux-devtools/extension": "3.2.5",
        "@reduxjs/toolkit": "1.9.7",
        "@rio-cloud/rio-notifications-component": "0.2.5",
        "@rio-cloud/rio-session-expired-info": "0.9.15",
        "@rio-cloud/rio-uikit": "0.16.3",
        "@rio-cloud/rio-user-menu-component": "2.0.4",
        "@sentry/browser": "7.73.0",
        "events": "3.3.0",
        "iframe-resizer-react": "1.1.0",
        "lodash": "4.17.21",
        "oidc-client-ts": "2.3.0",
        "qs": "6.11.2",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-hook-form": "7.47.0",
        "react-intl": "6.4.7",
        "react-redux": "8.1.3",
        "react-router": "6.16.0",
        "react-router-dom": "6.16.0"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "6.1.3",
        "@testing-library/react": "14.0.0",
        "@testing-library/user-event": "14.5.1",
        "@types/lodash": "4.14.199",
        "@types/node": "18.18.3",
        "@types/qs": "6.9.8",
        "@types/react": "18.2.25",
        "@types/react-dom": "18.2.10",
        "@types/react-redux": "7.1.27",
        "@typescript-eslint/eslint-plugin": "6.7.4",
        "@typescript-eslint/parser": "6.7.4",
        "@vitejs/plugin-react": "4.1.0",
        "cypress": "13.3.0",
        "eslint": "8.50.0",
        "eslint-config-prettier": "9.0.0",
        "eslint-plugin-compat": "4.2.0",
        "eslint-plugin-import": "2.28.1",
        "eslint-plugin-jest": "27.4.2",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-react": "7.33.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-testing-library": "6.0.2",
        "msw": "1.3.2",
        "pre-commit": "1.2.2",
        "prettier": "3.0.3",
        "rimraf": "5.0.5",
        "rollup-plugin-visualizer": "5.9.2",
        "start-server-and-test": "2.0.1",
        "typescript": "5.2.2",
        "vite": "4.4.11",
        "vitest": "0.34.6",
        "vitest-fetch-mock": "0.2.2"
    },
    "overrides": {
        "d3-color": "3.1.0"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "pre-commit": [
        "format-code-on-commit"
    ],
    "msw": {
        "workerDirectory": "public"
    }
}
