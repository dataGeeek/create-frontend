{
    "name": "CREATE_RIO_FRONTEND_appName",
    "description": "A frontend based on @rio-cloud/create-frontend",
    "private": true,
    "version": "1.0.0",
    "scripts": {
        "start": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "test": "vitest run",
        "test:ci": "vitest run --reporter=junit --outputFile.junit=./results/junit.xml",
        "test-dev": "vitest",
        "coverage": "vitest run --coverage",
        "lint": "eslint src",
        "lint-fix": "npm run lint -- --fix",
        "audit": "npm audit --production --registry https://registry.npmjs.org --parseable",
        "format-code": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
        "format-code-on-commit": "npm run format-code && git add",
        "pull-translations": "phrase pull --access_token $PHRASEAPP_ACCESS_TOKEN && npm run merge-translations",
        "merge-translations": "nodenv ./mergeTranslations.js",
        "cypress-open": "cypress open",
        "cypress-run": "cypress run",
        "cypress-ci": "start-server-and-test start http://localhost:3000 cypress-run"
    },
    "dependencies": {
        "@redux-devtools/extension": "3.2.5",
        "@reduxjs/toolkit": "^1.9.5",
        "@rio-cloud/rio-notifications-component": "0.2.4",
        "@rio-cloud/rio-session-expired-info": "0.9.14",
        "@rio-cloud/rio-uikit": "^0.16.3",
        "@rio-cloud/rio-user-menu-component": "2.0.3",
        "@sentry/browser": "^7.58.1",
        "events": "3.3.0",
        "iframe-resizer-react": "1.1.0",
        "lodash": "4.17.21",
        "oidc-client-ts": "^2.2.4",
        "qs": "^6.11.2",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-hook-form": "^7.45.1",
        "react-intl": "^6.4.4",
        "react-redux": "^8.1.1",
        "react-router": "^6.14.1",
        "react-router-dom": "^6.14.1"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "5.16.5",
        "@testing-library/react": "14.0.0",
        "@testing-library/user-event": "14.4.3",
        "@types/lodash": "^4.14.195",
        "@types/node": "^18.16.19",
        "@types/qs": "6.9.7",
        "@types/react": "^18.2.15",
        "@types/react-dom": "^18.2.7",
        "@types/react-redux": "7.1.25",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "@vitejs/plugin-react": "^4.0.3",
        "cypress": "^12.17.1",
        "eslint": "^8.45.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-compat": "^4.1.4",
        "eslint-plugin-import": "2.27.5",
        "eslint-plugin-jest": "^27.2.3",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-react": "7.32.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-testing-library": "^5.11.0",
        "msw": "^1.2.2",
        "nodenv": "0.1.0",
        "pre-commit": "1.2.2",
        "prettier": "^3.0.0",
        "rollup-plugin-visualizer": "^5.9.2",
        "start-server-and-test": "2.0.0",
        "typescript": "4.9.4",
        "vite": "^4.4.4",
        "vitest": "^0.33.0",
        "vitest-fetch-mock": "0.2.2"
    },
    "overrides": {
        "d3-color": "3.1.0"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        "last 2 versions",
        "last 5 Chrome versions",
        "Firefox >= 90",
        "Edge >= 95",
        "Safari >= 14"
    ],
    "pre-commit": [
        "format-code-on-commit"
    ],
    "msw": {
        "workerDirectory": "public"
    }
}
